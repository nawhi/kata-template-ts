  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samfirminger/allergies.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
