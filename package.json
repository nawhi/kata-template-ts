      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^test/(.*)$": "<rootDir>/test/$1"
    }
  },
  "homepage": "https://github.com/samfirminger/allergies#readme",
  "devDependencies": {
    "@types/jest": "^27.4.1",
